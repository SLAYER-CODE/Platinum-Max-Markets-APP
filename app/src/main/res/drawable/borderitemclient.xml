<?xml version="1.0" encoding="utf-8"?>
<inset xmlns:android="http://schemas.android.com/apk/res/android">

    <layer-list xmlns:android="http://schemas.android.com/apk/res/android">
        <item>
            <shape>
                <solid android:color="@color/md_black_1000">
                </solid>
                <corners android:radius="26dp" >
                </corners>
            </shape>
        </item>
        <item>
            <shape android:shape="rectangle" >
                <!--                    <corners android:radius="50dp" />-->
                <gradient
                    android:angle="90"
                    android:centerX="40%"
                    android:centerColor="@color/zxing_transparent"
                    android:startColor="#007214"
                    android:endColor="@color/zxing_transparent"
                    android:type="linear"
                    />
                <corners android:radius="35dp"/>
                <solid android:color="#006755"/>
                <gradient
                    android:angle="180"
                    android:centerX="60%"
                    android:centerColor="@color/zxing_transparent"
                    android:startColor="#088F1E"
                    android:endColor="@color/zxing_transparent"
                    android:type="linear"
                    />
                <stroke
                    android:width="3.5dp"
                    android:color="#ABF6D8"
                    />
            </shape>
        </item>

        <item android:gravity="center">

            <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
                <corners android:radius="35dp"/>
                <solid android:color="#660090BB" />
                <padding android:left="10dp" android:right="10dp" android:top="10dp" android:bottom="10dp"/>

                <stroke
                    android:dashWidth="60px"
                    android:dashGap="50px"
                    android:width="20dp"/>
                <stroke android:width="10dp" android:color="#45CA7A"/>


            </shape>


        </item>

        <item>
            <vector android:height="30dp" android:tint="#00A410"
                android:viewportHeight="24" android:viewportWidth="24"
                android:width="30dp" xmlns:android="http://schemas.android.com/apk/res/android">
                <path android:fillColor="@android:color/white" android:pathData="M19,13h-6v6h-2v-6H5v-2h6V5h2v6h6v2z"/>
            </vector>
        </item>

    </layer-list>
</inset>